{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-multi-assign": "off",
    "no-return-await": "off",
    "no-undef": "off",
    "no-console": "off",
    "consistent-return": "off",
    "func-names": "off",
    "no-param-reassign": "off",
    "no-shadow": "off",
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ]
  }
}
